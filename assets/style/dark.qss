QMainWindow {
    background: #303030;
}

QDialog {
    background: #303030;
}

QToolBar {
    border-radius: 8px;
    background: #3A3A3A;
    font-weight: bold;
    spacing: 15px;
    padding: 5px, 0, 5px, 0
}

QToolBar::item {
    spacing: 20px;
}

QToolBar::separator:horizontal {
    image: url('assets/images/Hsepartoolbar.png');
    padding: 4px;
}

QToolButton {
    background: #3A3A3A;
    border-radius: 5px;
    padding: 6px;
}

QToolButton::checked {
    background: #3574F0;
}

QToolButton::hover {
    background: #4D4D4D;
}

QLabel {
    color: #F0F0F0;
    font-family: 'segoe ui';
    font-size: 12px;
}

QScrollArea {
    background: #303030;
    border: 0px;
}

QAbstractSpinBox {
    background-color: rgba(58, 58, 58, 240);
    color: #F0F0F0;
    border-radius: 5px;
    min-width: 30px;
    min-height: 10px;
    padding: 4.5px;
}

QAbstractSpinBox:up-button {
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: top right;
}

QAbstractSpinBox:down-button {
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: bottom right;
}

QAbstractSpinBox::up-arrow,QAbstractSpinBox::up-arrow:disabled,QAbstractSpinBox::up-arrow:off {
    image: url('assets/images/up_arrow_disabled.png');
    width: 8px;
    height: 8px;
}
QAbstractSpinBox::up-arrow:hover {
    image: url('assets/images/up_arrow.png');
}

QAbstractSpinBox::down-arrow,QAbstractSpinBox::down-arrow:disabled,QAbstractSpinBox::down-arrow:off {
    image: url('assets/images/down_arrow_disabled.png');
    width: 8px;
    height: 8px;
}

QAbstractSpinBox::down-arrow:hover {
    image: url('assets/images/down_arrow.png');
}

QPushButton {
    background: rgba(58, 58, 58, 240);
    font-size: 12px;
    font-family: 'segoe ui';
    padding: 5px;
    color: #f0f0f0;
    border-radius: 5px;
}

QPushButton::hover {
    background: #3060B0;
}

QPushButton::pressed {
    background: #3574F0;
}

QPushButton::checked {
    background: #3574F0;
}

QProgressBar {
    border-radius: 3px;
    max-width: 300px;
    min-width: 300px;
    max-height: 6px;
    min-height: 6px;
    text-align: center;
    background: #3A3A3A;
    color: #fdfdfd;

}

QProgressBar::chunk {
    background: #3574F0;
    width: 3px;
    margin: 1px;
    border-radius: 3px;
}

QListWidget {
    color: #F0F0F0;
    font-size: 13px;
    background-color: rgba(77, 77, 77, 70);
    border: 0px;
    padding: 10px;
    spacing: 15px;
    text-align: center;
}

QListWidget::item{
    border-radius: 5px;
    text-align: center;
    padding: 5px;
    spacing: 10px;
}


QListWidget::item::selected{
    background: #303030;
    outline: 0px;
}

QListWidget::item::hover{
    background: #3a3a3a;
}

#list::item::hover{
    background: #4d4d4d;
}

QRadioButton, QCheckBox {
    color: #F0F0F0;
    font-size: 13px;
}

#loading_pages {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
    min-width: 320px;
    max-width: 320px;
    min-height: 80px;
    max-height: 80px;
}

#preview_lay {
    background: #303030;
    max-width: 140px;
    min-width: 140px;
    border-radius: 5px;
}

#title {
    font-family: 'segoe ui';
    font-size: 17px;
    color: white;
    font-weight: bold;
}

#pageNum {
    background: transparent;
    font-family: 'segoe ui';
    font-size: 12px;
}

#title_1 {
    font-family: 'segoe ui';
    font-size: 30px;
    color: white;
}

#topic {
    font-family: 'segoe ui';
    font-size: 18px;
    color: white;
    font-weight: bold;
}

#obj_1 {
    background: #4D4D4D;
    border-radius: 8px;
}

#go_to {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
    min-width: 160px;
    max-width: 160px;
    min-height: 39px;
    max-height: 39px;
}

#toggle_pane {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
    min-width: 80px;
    max-width: 80px;
    min-height: 45px;
    max-height: 45px;
}

#menu_panel {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
}

#action_btn {
    background: transparent;
    min-width: 20px;
    max-width: 20px;
    min-height: 20px;
    max-height: 20px;
    border-radius: 5px;
    padding: 4px;
}

#action_btn::hover {
    background: #303030;
}

#action_btn::checked {
    background: #3574F0;
}

#view_panel {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
    min-width: 250px;
    max-width: 250px;
    min-height: 45px;
    max-height: 45px;
}

#draw_btn::hover {
    background: rgba(58, 58, 58, 150);
}

#draw_btn::checked {
    background: #3574F0;
}

#info {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
    min-width: 300px;
    max-width: 300px;
    min-height: 280px;
    max-height: 280px;
}

#recent {
    background: rgba(77, 77, 77, 240);
    border: 2px solid rgba(58, 58, 58, 240);
    border-radius: 8px;
    min-width: 300px;
    max-width: 300px;
    min-height: 290px;
    max-height: 290px;
}

#preview {
    border-radius: 5px;
    padding: 2px;
}

#preview::hover {
    background: #3060B0;
}

#wlc {
    font-size: 35px;
    color: white;
    font-weight: bold;
    max-width: 500px;
    min-width: 500px;
}

#setBtn{
    max-width: 25px;
    min-width: 25px;
    max-height: 25px;
    min-height: 25px;
}

#topic_lbl{
    font-family: 'segoe ui';
    font-size: 23px;
    color: white;
    font-weight: bold;
    max-width: 140px;
    min-width: 140px;
}

#widget {
    max-width: 160px;
    min-width: 160px;
}

#button {
    font-size: 15px;
    border: 1px solid #fdfdfd;
    border-radius: 5px;
    max-width: 45px;
    min-width: 45px;
}